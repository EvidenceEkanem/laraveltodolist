{
    "name": "evidenceekanem/laraveltodolist",
    "description": ":A Laravel package that adds task management functionality to an app",
    "license": "MIT",
    "authors": [
        {
            "name": "evidenceekanem",
            "email": "ekanemevidence@gmail.com",
            "homepage": "https://github.com/evidenceekanem/"
        }
    ],
    "homepage": "https://github.com/evidenceekanem/laraveltodolist",
    "keywords": ["Laravel", "laravelTodoList"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "evidenceekanem\\laravelTodoList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "evidenceekanem\\laravelTodoList\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "evidenceekanem\\laravelTodoList\\laravelTodoListServiceProvider"
            ],
            "aliases": {
                "laravelTodoList": "evidenceekanem\\laravelTodoList\\Facades\\laravelTodoList"
            }
        }
    }
}
